		//Ejercicio 4
		int randomNum = 0;
		int[][] matriz = new int[3][3];
		List<Integer> numList = new ArrayList<Integer>();
		for(int x=0;x<3;x++) {
			for(int y=0;y<3;y++) {
				do {
				randomNum = ThreadLocalRandom.current().nextInt(0,100+1);
				}while(numList.contains(randomNum));
				numList.add(randomNum);
				matriz[x][y] = randomNum;
