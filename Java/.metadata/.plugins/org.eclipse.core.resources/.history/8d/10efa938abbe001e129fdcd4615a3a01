package com.integrador.spring.models;

import java.io.Serializable;

import jakarta.persistence.Column;
import jakarta.persistence.Embeddable;

@SuppressWarnings("serial")
@Embeddable
public class ProductsPerOrderKey implements Serializable {
	@Column
	Integer productId;
	
	@Column
	Integer courseId;

	public ProductsPerOrderKey() {
		
	}
	
	public ProductsPerOrderKey(Integer studentId, Integer courseId) {
		super();
		this.productId = studentId;
		this.courseId = courseId;
	}

	public Integer getStudentId() {
		return productId;
	}

	public void setStudentId(Integer studentId) {
		this.productId = studentId;
	}

	public Integer getCourseId() {
		return courseId;
	}

	public void setCourseId(Integer courseId) {
		this.courseId = courseId;
	}
	
	
}
